/* tf-template */
#wrap { background:#fff; }
#news-wrap { background:#fff; }
#news-wrap h1{margin:0 14px;}
#news-body { font-size:16px; background:#fff; position:relative;}
#news-title {
    padding:18px 14px 6px 14px;
    font-weight:bold;
}
#news-info { font-size:14px; color:#9b9b9b; margin:0 14px; padding-bottom:10px; border-bottom:1px solid #c9c9c9; }
#news-info span { display:inline-block; }
#news-info-from { margin-left:1em; }
#news-content {
    padding:16px 14px;
	/*overflow:hidden;*/
    word-wrap:break-word;
	word-break:break-all;
	line-height:1.5em;
	display:block;
	float:left;
}
#news-content img {
    -webkit-box-sizing:border-box;
    max-width:100%;
    border:1px solid #e0e0e0 !important;
    padding:2px;
}
#news-pages { font-size:18px; text-align:center; }
.news-pages-full {
    height:auto !important;
}
#news-pages *{
    display:inline-block;
    margin:8px 8px;
}

.news-other {
	margin:8px 0 8px 0;
	-webkit-box-sizing:border-box;
	border-bottom:1px solid #ccc;
	/*-webkit-border-radius:4px;*/
}
.news-other-title {
	/*background:url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAMAAAADCAIAAADZSiLoAAAAGXRFWHRTb2Z0d2FyZQBBZG9iZSBJbWFnZVJlYWR5ccllPAAAAB9JREFUeNpiuHjx4jcwYARiBgaG27dvMwBZEGGAAAMASBYV4yuOaDAAAAAASUVORK5CYII=) repeat;*/
	background-color:#ededed;
	min-height:36px;
	display:-webkit-box;
	-webkit-box-sizing:border-box;
	-webkit-box-pack:justify;
	-webkit-box-align:center;
	padding:0 16px;
	position:relative;
	/*-webkit-border-radius:3px 3px 0 0;
	-webkit-box-shadow:inset 1px 1px 0 rgba(255,255,255,1), inset -1px -1px 0 rgba(255,255,255,1);*/
    font-weight:bold;
}
.news-other-title>.icon-sprite {
    width:20px;
    height:14px;
	background-position:-79px -70px;
    -webkit-transform:rotate(-90deg);
    display:none;
}
.news-other-list {
    margin:0;
    list-style:none;
    line-height:1.6em;
}
.news-other-list a {
    /*color:#334f91 !important;*/
    text-decoration:none;
    text-align:left;
    color:#000;
    padding:8px 14px;
    -webkit-box-sizing:border-box;
    width:100%;
    height:100%;
    overflow: hidden;text-overflow: ellipsis;width: 100%;display: block;white-space: nowrap;
}
.news-other-list li {
    min-height:32px;
    display:-webkit-box;
    -webkit-box-pack:begin;
    -webkit-box-align:center;
    border-bottom:1px solid #e6e7e7
}
.news-other-list li:last-child {
    border-bottom:none;
}
.news-other-list span {
    color:#a2a2a2;
    text-align:right;
    max-width:130px;
}


#footer {
    border-top:none !important;
    -webkit-box-shadow:none !important;
}
#news-footer {
    border-top:1px solid #e6e7e7;
